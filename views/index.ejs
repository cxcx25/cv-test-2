                                    <!DOCTYPE html>
                                    <html lang="en">
                                    <head>
                                        <meta charset="UTF-8">
                                        <meta name="viewport" content="width=device-width, initial-scale=1.0">
                                        <title>Joseph Rey Marilla - Blog</title>
                                        <link rel="stylesheet" href="/styles.css">
                                    </head>
                                    <body>
                                        <header>
                                            <h1>Joseph Rey Marilla's Blog</h1>
                                            <!-- Add user login/logout logic here -->
                                        </header>

                                        <nav>
                                            <ul>
                                                <li><a href="/">Home</a></li>
                                                <li><a href="#blogs">Blogs</a></li>
                                            </ul>
                                        </nav>

                                        <div class="container">
                                            <h2>Blogs</h2>
                                            <ul>
                                                <% blogs.forEach(blog => { %>
                                                    <li>
                                                        <h3><%= blog.title %></h3>
                                                        <p><%= blog.content %></p>
                                                    </li>
                                                <% }) %>
                                            </ul>
                                        </div>

                                        <footer>
                                            <p>&copy; 2024 Joseph Rey Marilla. All rights reserved.</p>
                                        </footer>
                                    </body>
                                    </html>
